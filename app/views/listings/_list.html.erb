<% @listings.each do |listing|%>
<!-- Project One -->
  <div class="row">
      <div class="col-sm-7">
          <a href="portfolio-item.html">
              <img class="img-responsive img-hover" src="http://placehold.it/700x300" alt="">
          </a>
      </div>
      <div class="col-sm-5">
          <h3><%= link_to listing.title, listing %></h3>
          <h4>Rate: <%= listing.price%></h4>
          <p>Description: <%= listing.description %></p>
          <%= link_to "View more", listing, class: "btn btn-primary"%>
      </div>
  </div>
<!-- /.row -->

<hr>
<% end %>
